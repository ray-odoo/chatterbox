<odoo>
    <t t-name="documents_spreadsheet.DocumentSelectorPanel" t-inherit="spreadsheet_edition.SpreadsheetSelectorDialog" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('o-sp-dialog-headers')]" position="inside">
            <div class="o-sp-dialog-header" t-att-class="{ active: state.panel === 'spreadsheets' }" t-on-click="() => this.activatePanel('spreadsheets')">
                    Spreadsheets
            </div>
        </xpath>
        <xpath expr="//div[hasclass('o-sp-dialog-content')]" position="inside">
            <DocumentsSelectorPanel t-if="state.panel === 'spreadsheets'" onSpreadsheetSelected.bind="onSpreadsheetSelected" />
        </xpath>
    </t>
</odoo>