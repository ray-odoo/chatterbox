<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

<t t-name="hr_recruitment_reports.PerformanceRankingDashboard"
    t-inherit="web_dashboard.DashboardView"
    t-inherit-mode="primary"
    owl="1">
    <xpath expr="//Layout" position="inside">
        <div class="o_team_ranking_dashboard">
            <div class="o_team_ranking_list">
                <div class="o_team_ranking_item"
                    t-foreach="state.ranking_list"
                    t-as="ranking"
                    t-key="ranking.title">
                    <DashboardRanking ranked_list="ranking"/>
                </div>
            </div>
        </div>
    </xpath>
</t>

</templates>
